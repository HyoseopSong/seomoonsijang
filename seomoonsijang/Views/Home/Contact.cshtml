@{
    ViewBag.Title = "Contact";
}
<h2>@ViewBag.Title</h2>

@using (Html.BeginForm("Contact", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div>
        <input type="file" name="file" /><br />

        <input type="submit" value="Upload" />
        
        <h3>@ViewBag.Message</h3>

    </div>
}